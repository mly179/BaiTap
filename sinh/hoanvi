#include <bits/stdc++.h>

using namespace std;

int a[100],n;
bool check[100];
void init(){
	cin >> n;
    for(int i=1;i<=n;i++)
        a[i]=i;
}
bool isEnd(){
    for(int i=1;i<=n;i++){
        if(a[i]!=n-i+1) return false;
    }
    return true;
}

void display(){
    for(int i=1;i<=n;i++) cout << a[i]<<" ";
    cout << endl;
}
void gene () {
     int i; 
    for ( i=n;i>1;i--) {
        if (a[i-1]<a[i]) break;
    }
    for (int j=n;j>=i;j--) {
        if (a[j]>a[i-1]) {
            swap (a[i-1],a[j]);
            sort(a+i,a+n+1);
            break;
        }
    }

}

int main(){
    init();
    while(!isEnd()){
    	display();
    	gene();
	}
	display();
    return 0;
}
